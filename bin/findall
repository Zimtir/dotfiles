#!/Library/Frameworks/Python.framework/Versions/3.8/bin/python3

import re
import sys

def main():
    path = sys.argv[1]
    regex = r'{}'.format(sys.argv[2])

    with open(path, "r") as file:
        text = file.read()

    matches = re.findall(regex, text)

    if len(matches) == 0:
        print("No matches with given regex pattern.")
        return
    
    for match in matches:
        print(match)

if __name__ == "__main__":
    main()    
